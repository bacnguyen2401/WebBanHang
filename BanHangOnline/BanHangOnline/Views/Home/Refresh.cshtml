@model BanHangOnline.Models.ThongKeModel
@if (Model != null)
{
    <div class="thongke">
        <table>
            <tr>
                <td>Hôm nay: </td>
                <td> @Model.HomNay</td>
            </tr>

            <tr>
                <td>Hôm qua: </td>
                <td> @Model.HomQua</td>
            </tr>

            <tr>
                <td>Tuần này: </td>
                <td> @Model.TuanNay</td>
            </tr>
            <tr>

                <td>Người online: </td>
                <td> ViewBag.visitors_online</td>
            </tr>
        </table>
    </div>
}